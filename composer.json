{
    "name": "corneltek\/preview",
    "require": {
        "symfony\/yaml": "~2",
        "twig\/twig": "~1",
        "twig\/extensions": "~1",
        "corneltek\/fileutil": "~1",
        "corneltek\/twig-markdown": "*",
        "corneltek\/scss-runner": "~1.3",
        "corneltek\/cliframework": "*",
        "corneltek\/configkit": "*",
        "corneltek\/locale": "~1"
    },
    "require-dev": {
        "corneltek\/phpunit-testmore": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "michelf\/php-markdown",
                "version": "1.0",
                "dist": {
                    "url": "https:\/\/github.com\/michelf\/php-markdown\/zipball\/extra",
                    "type": "zip"
                },
                "autoload": {
                    "files": [
                        "markdown.php"
                    ]
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "Corneltek\\Preview": "src"
        }
    },
    "version": "2.2.5"
}
